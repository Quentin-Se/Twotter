CAAC,CAAC,CAAC,CAAC;CACN,CAAC,CAAC,CAAC,CAAC,CAAC","file":"/Users/Quentin/Documents/Github/Twotter/src/components/UserProfile.vue","sourceRoot":"","sourcesContent":["<template>\n\t<div class=\"user-profile\">\n\t\t<div class=\"user-profile__sidebar\">\n\t\t\t<div class=\"user-profile__user-panel\">\n\t\t\t\t<h1 class=\"user-profile__username\">@{{ user.username }}</h1>\n\t\t\t\t<div class=\"user-profile__admin-badge\" v-if=\"user.isAdmin\">\n\t\t\t\t\tAdmin\n\t\t\t\t</div>\n\t\t\t\t<div class=\"user-profile__follower-count\">\n\t\t\t\t\t<strong>Followers: </strong> {{ followers }}\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t\t<CreateTwootPanel @add-twoot=\"addTwoot\" />\n\t\t</div>\n\t\t<div class=\"user-profile__twoots-wrapper\">\n\t\t\t<TwootItem\n\t\t\t\tv-for=\"twoot in user.twoots\"\n\t\t\t\t:key=\"twoot.id\"\n\t\t\t\t:username=\"user.username\"\n\t\t\t\t:twoot=\"twoot\"\n\t\t\t\t@favourite=\"toggleFavourite\"\n\t\t\t/>\n\t\t</div>\n\t</div>\n</template>\n\n<script>\nimport TwootItem from './TwootItem';\nimport CreateTwootPanel from './CreateTwootPanel';\n\nexport default {\n\tname: 'UserProfile',\n\tcomponents: { CreateTwootPanel, TwootItem },\n\tdata() {\n\t\treturn {\n\t\t\tnewTwootContent: '',\n\t\t\tselectedTwootType: 'instant',\n\t\t\ttwootTypes: [\n\t\t\t\t{\n\t\t\t\t\tvalue: 'draft',\n\t\t\t\t\tname: 'Draft',\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tvalue: 'instant',\n\t\t\t\t\tname: 'InstantTwoot',\n\t\t\t\t},\n\t\t\t],\n\t\t\tfollowers: 0,\n\t\t\tuser: {\n\t\t\t\tid: 1,\n\t\t\t\tusername: '_QuentinSenelas',\n\t\t\t\tfirstName: 'Quentin',\n\t\t\t\tlastName: 'Sénélas',\n\t\t\t\temail: 'quentin.senelas@gmail.com',\n\t\t\t\tisAdmin: true,\n\t\t\t\ttwoots: [\n\t\t\t\t\t{\n\t\t\t\t\t\tid: 1,\n\t\t\t\t\t\tcontent: 'Twotter is Amazing!',\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\tid: 2,\n\t\t\t\t\t\tcontent: 'Did you all know that Earth is actually a triangle ?!',\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\tid: 3,\n\t\t\t\t\t\tcontent: \"Go attack Lili Jean's twotter !\",\n\t\t\t\t\t},\n\t\t\t\t],\n\t\t\t},\n\t\t};\n\t},\n\twatch: {\n\t\tfollowers(newFollowerCount, oldFollowerCount) {\n\t\t\tif (oldFollowerCount < newFollowerCount) {\n\t\t\t\tconsole.log(`${this.user.username} has gained a follower !`);\n\t\t\t}\n\t\t},\n\t},\n\tcomputed: {\n\t\tnewTwootCharacterCount() {\n\t\t\treturn this.newTwootContent.length;\n\t\t},\n\t},\n\tmethods: {\n\t\tfollowUser() {\n\t\t\tthis.followers++;\n\t\t},\n\t\ttoggleFavourite(id) {\n\t\t\tconsole.log(`Favourited Tweet #${id}`);\n\t\t},\n\t\tcreateNewTwoot() {\n\t\t\tif (this.newTwootContent && this.selectedTwootType !== 'draft') {\n\t\t\t\tthis.user.twoots.unshift({\n\t\t\t\t\tid: this.user.twoots.length + 1,\n\t\t\t\t\tcontent: this.newTwootContent,\n\t\t\t\t});\n\t\t\t\tthis.newTwootContent = '';\n\t\t\t}\n\t\t},\n\t},\n\tmounted() {\n\t\tthis.followUser();\n\t},\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.user-profile {\n\tdisplay: grid;\n\tgrid-template-columns: 1fr 3fr;\n\twidth: 100%;\n\tpadding: 50px 5%;\n\tbox-sizing: border-box;\n\n\t.user-profile__user-panel {\n\t\tdisplay: flex;\n\t\tflex-direction: column;\n\t\tmargin-right: 20px;\n\t\tpadding: 20px;\n\t\tbackground-color: white;\n\t\tborder-radius: 5px;\n\t\tborder: 1px solid #dfe3e8;\n\n\t\th1 {\n\t\t\tmargin: 0;\n\t\t}\n\n\t\t.user-profile__admin-badge {\n\t\t\tbackground-color: rebeccapurple;\n\t\t\tcolor: white;\n\t\t\tborder-radius: 5px;\n\t\t\tmargin-right: auto;\n\t\t\tmargin-top: 10px;\n\t\t\tmargin-bottom: 10px;\n\t\t\tpadding: 0 10px;\n\t\t\tfont-weight: bold;\n\t\t}\n\n\t\t.user-profile__create-twoot {\n\t\t\tpadding-top: 20px;\n\t\t\tdisplay: flex;\n\t\t\tflex-direction: column;\n\n\t\t\t&.--excedeed {\n\t\t\t\tcolor: red;\n\t\t\t\tborder-color: red;\n\n\t\t\t\tbutton {\n\t\t\t\t\tbackground-color: red;\n\t\t\t\t\tborder: none;\n\t\t\t\t\tcolor: white;\n\t\t\t\t}\n\t\t\t}\n\n\t\t\t&-type {\n\t\t\t\tpadding: 10px 0;\n\t\t\t}\n\t\t}\n\t}\n\n\t.user-profile__twoots-wrapper {\n\t\tdisplay: grid;\n\t\tgrid-gap: 10px;\n\t}\n}\n</style>\n"]}]}